{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dbmohapatra-df"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorage1_AWDW_Tables_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1_AWDW_Tables'"
		},
		"AzureDataLakeStorage1_Parquet_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1_Parquet'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"Customer_Output_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Customer_Output'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"SqlServer2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer2'"
		},
		"SqlServerCustomer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerCustomer'"
		},
		"AzureDataLakeStorage1_AWDW_Tables_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mybucket12.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage1_Parquet_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mybucket12.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Key-1234.vault.azure.net/"
		},
		"Customer_Output_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mybucket12.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dbmohapatra-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage1_AWDW_Tables_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_AWDW_Tables_accountKey')]"
					},
					"AzureDataLakeStorage1_Parquet_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_Parquet_accountKey')]"
					},
					"Customer_Output_accountKey": {
						"value": "[parameters('Customer_Output_accountKey')]"
					},
					"SqlServer2_connectionString": {
						"value": "[parameters('SqlServer2_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"SqlServerCustomer_connectionString": {
						"value": "[parameters('SqlServerCustomer_connectionString')]"
					},
					"AzureDataLakeStorage1_AWDW_Tables_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_AWDW_Tables_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_Parquet_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_Parquet_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"Customer_Output_properties_typeProperties_url": {
						"value": "[parameters('Customer_Output_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "dbmohapatra-df_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dbmohapatra-df_ArmTemplate_0')]"
			]
		}
	]
}